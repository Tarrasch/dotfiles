" Turn on line numbering. Turn it off with "set nonu" 
set nu 

" Set syntax on
"let mysyntaxfile = "~/jr.vim"
" syntax on

"if 1
"    set syntax=java
"endif


" Indent automatically depending on filetype
filetype indent on
set autoindent

" Case insensitive search
set ic

" Higlhight search
set hls

" Wrap text instead of being on one line
set lbr

" Change colorscheme from default to delek
colorscheme delek

set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
